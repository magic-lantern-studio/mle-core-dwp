			<xs:enumeration value="SFColorRGBA"/>
			<xs:enumeration value="MFColorRGBA"/>
			<xs:enumeration value="SFDouble"/>
			<xs:enumeration value="MFDouble"/>
			<xs:enumeration value="SFFloat"/>
			<xs:enumeration value="MFFloat"/>
